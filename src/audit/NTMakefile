RELDIR=audit
!INCLUDE ..\config\NTMakefile.$(SYS_NAME)
!INCLUDE ..\config\NTMakefile.version

INCFILEDIR = $(DESTDIR)\include\afs

INCFILES =\
	$(INCFILEDIR)\audit.h

# build afsaudit.lib

LIBFILE = $(DESTDIR)\lib\afs\afsaudit.lib

LIBOBJS =\
	$(OUT)\audit.obj \
        $(OUT)\audit-file.obj

$(LIBFILE): $(LIBOBJS)
	$(LIBARCH)

install:  $(INCFILES)  $(LIBFILE)

clean::
	$(DEL) $(INCFILES)