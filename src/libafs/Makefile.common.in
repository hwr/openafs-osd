COMMON_INCLUDE = -I. -I.. -I../nfs \
	-I$(TOP_SRCDIR)/crypto/hcrypto/kernel \
	-I${TOP_SRCDIR} \
	-I${TOP_SRCDIR}/afs \
	-I${TOP_SRCDIR}/afs/${MKAFS_OSTYPE} \
	-I${TOP_SRCDIR}/config \
	-I${TOP_SRCDIR}/rx/${MKAFS_OSTYPE} \
	-I${TOP_SRCDIR}/external/heimdal \
