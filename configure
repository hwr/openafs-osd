echo
echo "**************************************"
echo configure summary
echo
if test $LIB_curses; then :

echo "LIB_curses :                $LIB_curses"
else

echo "XXX LIB_curses  not found! not building scout and afsmonitor!"

fi
echo
echo "**************************************"

